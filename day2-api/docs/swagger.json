{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample server for a Swagger API example.",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/": {
            "get": {
                "description": "Returns a hello message",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "hello"
                ],
                "summary": "Hello endpoint",
                "responses": {
                    "200": {
                        "description": "Hello, World!",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/v2": {
            "get": {
                "description": "Returns a hello message from resource",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "hello"
                ],
                "summary": "Hello2 endpoint",
                "responses": {
                    "200": {
                        "description": "Hello, World v2!",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}